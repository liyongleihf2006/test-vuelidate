{
  "name": "my-vue-webpack",
  "version": "1.0.0",
  "author": "liyongleihf2006",
  "description": "vue webpack",
  "license": "ISC",
  "sideEffects": false,
  "scripts": {
    "bower": "bower install",
    "dll": "webpack --config ./webpack.dll.config.js",
    "start": "npm run bower&&npm run dll && webpack --config ./webpack.dev.config.js",
    "start:nomock": "npm run bower&&npm run dll && webpack --config ./webpack.dev-nomock.config.js",
    "server": "npm run bower&&npm run dll && webpack-dev-server --config ./webpack.dev.config.js  --inline --open",
    "server:nomock": "npm run bower&&npm run dll && webpack-dev-server --config ./webpack.dev-nomock.config.js  --inline --open",
    "build": "npm run bower&& webpack --config ./webpack.prod.config.js ",
    "debug:start": "npm run bower&& node --inspect --inspect-brk node_modules/webpack/bin/webpack --config ./webpack.dev.config.js",
    "debug:build": "npm run bower&& node --inspect --inspect-brk node_modules/webpack/bin/webpack --config ./webpack.prod.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "bower": "^1.8.8",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "ensure-script-loader": "^1.0.1",
    "extract-loader-path-correction": "^1.0.3",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  }
}
